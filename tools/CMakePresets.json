{
	"version": 6,
	"configurePresets": [
		{
			"name": "tools",
			"binaryDir": "build.tools",
			"cacheVariables": {
				"DEBUG": "ON",
				"SCOPE": "TOOLS"
			}
		}
	],
	"buildPresets": [
		{
			"name": "tools",
			"configurePreset": "tools",
			"targets": [
				"git-remote-interactive-proto",
				"git-remote-interactive-proto-attach",
				"git-remote-interactive-proto-runner"
			]
		},
		{
			"name": "run-git-remote-interactive-proto",
			"configurePreset": "tools",
			"targets": [
				"run-git-remote-interactive-proto"
			]
		}
	],
	"workflowPresets": [
		{
			"name": "tools",
			"steps": [
				{ "type": "configure", "name": "tools" },
				{ "type": "build", "name": "tools" }
			]
		},
		{
			"name": "run-git-remote-interactive-proto",
			"steps": [
				{ "type": "configure", "name": "tools" },
				{ "type": "build", "name": "run-git-remote-interactive-proto" }
			]
		}
	]
}
